void sort012(int a[], int n)
    {
        // coode here 
        int lo=0;
        int high=n-1;
        int mid=0;
        while(mid<=high)
        {
            switch(a[mid])
            {
                case 0: 
                        swap(a[lo++],a[mid++]);
                        break;
                case 1:
                        mid++;
                        break;
                case 2:
                        swap(a[mid],a[high--]);
                        break;
            }
        }
    }
    
    
